{
  "version": 1,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "src/**/*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/assets/(.*)", "dest": "/public/assets/$1" },
    { "src": "/css/(.*)", "dest": "/src/css/$1" },
    { "src": "/js/(.*)", "dest": "/src/js/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
